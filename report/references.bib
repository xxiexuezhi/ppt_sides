  file = {/home/strokach/Zotero/storage/GAGIB2W6/Cooper et al_2014_A biomolecular electrostatics solver using Python, GPUs and boundary elements.pdf;/home/strokach/Zotero/storage/5QCAG3GG/S0010465513003731.html}
