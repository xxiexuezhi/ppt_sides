  shortjournal = {PLOS Computational Biology},
  urldate = {2017-09-21},
  date = {2014-04-24},
  pages = {e1003569},
  keywords = {ENCoM,NMA},
  author = {Frappier, Vincent and Najmanovich, Rafael J.},
